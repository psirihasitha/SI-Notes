
#include <iostream>
#include <cmath>
using namespace std;

int powerof2(int n){
    return 1<<n;
}

int main() {
  cout << powerof2(5);
  return 0;
}

def ispowerof2(n):
    count = 0
    if n< 0 :
        return False
    else:
        while n>0:
            n = n & n-1
            count+=1
        return count == 1
print(ispowerof2(5))
